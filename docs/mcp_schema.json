{
    "tool_type": "json-api",
    "schema_version": "1.0",
    "base_url": "http://localhost:5656/api",
    "description": "API Logic Project: basic_demo",
    "resources": [
        {
            "name": "Customer",
            "path": "/Customer",
            "methods": [
                "GET",
                "PATCH",
                "POST",
                "DELETE"
            ],
            "fields": [
                "id",
                "name",
                "balance",
                "credit_limit"
            ],
            "filterable": [
                "id",
                "name",
                "balance",
                "credit_limit"
            ]
        },
        {
            "name": "Item",
            "path": "/Item",
            "methods": [
                "GET",
                "PATCH",
                "POST",
                "DELETE"
            ],
            "fields": [
                "id",
                "order_id",
                "product_id",
                "quantity",
                "amount",
                "unit_price"
            ],
            "filterable": [
                "id",
                "order_id",
                "product_id",
                "quantity",
                "amount",
                "unit_price"
            ]
        },
        {
            "name": "Order",
            "path": "/Order",
            "methods": [
                "GET",
                "PATCH",
                "POST",
                "DELETE"
            ],
            "fields": [
                "id",
                "notes",
                "customer_id",
                "date_shipped",
                "amount_total"
            ],
            "filterable": [
                "id",
                "notes",
                "customer_id",
                "date_shipped",
                "amount_total"
            ]
        },
        {
            "name": "Product",
            "path": "/Product",
            "methods": [
                "GET",
                "PATCH",
                "POST",
                "DELETE"
            ],
            "fields": [
                "id",
                "name",
                "unit_price"
            ],
            "filterable": [
                "id",
                "name",
                "unit_price"
            ]
        }
    ]
}